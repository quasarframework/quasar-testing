{
  "name": "@quasar/quasar-app-extension-testing-unit-jest",
  "version": "3.0.0-alpha.6",
  "license": "MIT",
  "description": "A Quasar App Extension for running Jest tests",
  "contributors": [
    "Paolo Caleffi",
    "Daniel Thompson-Yvetot"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/quasarframework/quasar-testing"
  },
  "scripts": {
    "build": "rm -rf dist && tsc --declaration --declarationDir dist/types && copyfiles -f src/helpers/stub.css dist",
    "deploy:alpha": "yarn build && yarn publish --tag alpha",
    "deploy:beta": "yarn build && yarn publish --tag beta",
    "deploy:latest": "yarn build && yarn publish --tag latest",
    "test:unit": "jest",
    "test:unit:watch": "jest --watch",
    "test": "snyk test && echo \"No test specified\" && exit 0"
  },
  "publishConfig": {
    "access": "public"
  },
  "main": "dist/index.js",
  "typings": "dist/types/index.d.ts",
  "engines": {
    "node": ">= 12.22.1",
    "npm": ">= 6.14.12",
    "yarn": ">= 1.17.3"
  },
  "// TODO:": "To support Jest 27 see https://github.com/vuejs/vue-jest/releases/tag/v27.0.0-alpha.2",
  "dependencies": {
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@vue/test-utils": "^2.0.0-rc.10",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "^26.6.3",
    "chai": "^4.3.4",
    "concurrently": "^6.2.0",
    "jest": "^26.6.3",
    "jest-serializer-vue": "^2.0.2",
    "jest-transform-stub": "^2.0.0",
    "json5": "^2.2.0",
    "loader-utils": "^1.2.3",
    "lodash-es": "^4.17.21",
    "ts-jest": "^26.5.6",
    "vue-jest": "^5.0.0-alpha.10"
  },
  "devDependencies": {
    "@types/lodash-es": "^4.17.4",
    "copyfiles": "^2.4.1",
    "quasar": "^2.0.0",
    "snyk": "^1.180.1",
    "vue": "^3.1.2"
  }
}
