{
  "env": {
    "test": {
      "presets": [    [
        "@babel/preset-env", {
          "node": "current",
          "loose": false,
          "useBuiltIns": "usage",
          "decoratorsLegacy": true
        }
      ]
      ]
    }
  },
  "presets": [
    [
      "@babel/preset-env", {
        "modules": false,
        "loose": false,
        "useBuiltIns": "usage"
      }
    ],
    [
      "@babel/preset-stage-2", {
        "modules": false,
        "loose": false,
        "useBuiltIns": true,
        "decoratorsLegacy": true
      }
    ]
  ],
  "plugins": [
    [
      "@babel/transform-runtime", {
        "polyfill": false,
        "regenerator": false
      }
    ]
  ],
  "compact": false,
  "comments": false
}
